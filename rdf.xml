<rdf:Property rdf:ID="country-name">
<rdfs:domain rdf:resource="#Address"/>
</rdf:Property>
<rdf:Property rdf:ID="min">
<rdfs:domain rdf:resource="#timeRange"/>
</rdf:Property>
<rdf:Property rdf:ID="max">
<rdfs:domain rdf:resource="#timeRange"/>
</rdf:Property>
